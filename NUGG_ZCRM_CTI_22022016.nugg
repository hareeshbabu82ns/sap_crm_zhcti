        EH_ONNEW( htmlb_event    = htmlb_event
                    htmlb_event_ex = htmlb_event_ex ).

      WHEN &apos;select&apos;.                                        &quot;#EC NOTEXT
        eh_onSelect(
                  htmlb_event    = htmlb_event
                  htmlb_event_ex = htmlb_event_ex ).

      WHEN &apos;sort&apos;.                                          &quot;#EC NOTEXT
        eh_onSort(
                  htmlb_event    = htmlb_event
                  htmlb_event_ex = htmlb_event_ex ).

      WHEN OTHERS.
    ENDCASE.
endmethod.</source>
